<Title>:
    name: "title_page"
    FloatLayout:
        Label:
            text: "BrainStormie"
            font_size: 48
            color: 0, 0, 0,1
            color: 255/255, 212/255, 212/255,1
        Button:
            background_color: 140/255, 100/255, 255/255
            text: "Start"
            color: 245/255, 136/255, 27/255
            size_hint: None,0.1
            width: "150dp"
            pos_hint: {"center_x": 0.5,"y":0.1}
            on_release: app.root.current = "intro"


<Intro_page>:
    name: "intro"
    FloatLayout:
        Image:
            source: "Intro_page.png"
            allow_stretch: True
            keep_ratio: True
            size_hint: 1,1
        Button:
            id: intro_btn
            text: "I understand"
            disabled: True
            size_hint: None,0.05
            width: "100dp"
            pos_hint: {"center_x": 0.65,"y":0.07}
            on_release: app.root.current = "index"
        ToggleButton:
            id: toggle
            text: "YES" if self.state == "down" else "NO"
            size_hint: None,0.05
            width: "50dp"
            pos_hint: {"center_x": 0.4,"y":0.07}
            on_release: intro_btn.disabled = False
            #on_press: intro_btn.disabled = True


<Index>:
    name: "index"
    Label:
        text: "INDEX"
        pos_hint: {"center_x": 0.5, "top": 1}
        size_hint: 1,0.18
        font_size: 48
        color: 0, 0, 0,1
    FloatLayout:
        orientation: "lr-tb"
        minimum_height: 100
        Button:
            text: "Chapter 1"
            size_hint: 0.3,0.07
            pos_hint: {"top": 0.8, "right": 0.45}
            background_color: 140/255, 100/255, 255/255
            on_release: app.root.current = 'Q01_pg'
        Button:
            text: "Chapter 2"
            size_hint: 0.3,0.07
            pos_hint: {"top": 0.8, "right": 0.85}
            background_color: 140/255, 100/255, 255/255
